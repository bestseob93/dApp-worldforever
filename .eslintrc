{
    "extends": [
        "react-app",
        "airbnb"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "function-paren-newline": 0,
        "comma-dangle": 0,
        "no-use-before-define": 0,
        "arrow-parens": 0,
        "no-param-reassign": 0,
        "no-console": 0
    }
}